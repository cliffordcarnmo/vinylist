<%- include("header") -%>

<div class="grid" data-masonry='{"percentPosition": true }'>
	<% catalog.forEach((record) => { %>
		<div class="grid-item p-1">
			<a href="/record/<%= record.id %>">
				<img class="record" src="<%= record.img %>" class="card-img-top rounded-1" alt="<%= record.artistname +" - "+ record.recordname %>">
			</a>
			<p class="card-footer"><a href="/artist/<%= record.artistid %>"><%= record.artistname %></a> - <a href="/record/<%= record.id %>"><%= record.recordname %></a></p>
		</div>
	<% }); %>
</div>

<%- include("footer") -%>